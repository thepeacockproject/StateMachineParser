            "Context": {
                "Number": 0,
                "Outfit": "caa37b77-90e2-4118-813b-b8dcf0ce0a6c"
            },
            "States": {
                "Start": {
                    "Disguised": [
                        {
                            "Condition": {
                                "$eq": ["$Value.RepositoryId", "$Outfit"]
                            },
                            "Actions": [
                                {
                                    "$set": ["$Number", 5]
                                }
                            ]
                        }
                    ],
                    "AmbientChanged": {
                        "Condition": {
                            "$eq": ["$Number", 5]
                        },
                        "Transition": "NumberIsFive"
                    }
                }
            }
        },
        "Inputs": [
            {
                "Name": "ContractStarted",
                "Value": null
            },
            {
                "Name": "Disguised",
                "Value": {
                    "RepositoryId": "caa37b77-90e2-4118-813b-b8dcf0ce0a6c"
                }
            },
            {
                "_comment": "why not",
                "Name": "AlexaUnderMoose",
                "Value": {}
            },
            {
                "Name": "AmbientChanged",
                "Value": {}
            }
        ]
    },
