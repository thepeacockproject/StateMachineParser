    "pushUniqueAsCondition": {
        "Definition": {
            "Context": {
                "Targets": [],
                "Target": "abc123",
                "Pacified": []
            },
            "States": {
                "Start": {
                    "Pacify": {
                        "Condition": {
                            "$pushunique": ["Pacified", "$Value.RepositoryId"]
                        },
                        "Transition": "OnePacified"
                    }
                },
                "OnePacified": {
                    "Pacify": {
                        "Condition": {
                            "$not": {
                                "$pushunique": [
                                    "Pacified",
                                    "$Value.RepositoryId"
                                ]
                            }
                        },
                        "Transition": "Success"
                    }
                }
            }
        },
        "Input": {
            "Name": "Pacify",
            "Value": {
                "RepositoryId": "abc123"
            }
        }
    }
